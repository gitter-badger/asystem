body
  font-family "Helvetica Neue Light", "HelveticaNeue-Light", "Helvetica Neue", Calibri, Helvetica, Arial, sans-serif
.sort
nav
  position relative

.notification
  position absolute;
  z-index 400
  right 8%
  top 12%
  width 300px
.details
  td
    border 1px solid #DDD;
.modal-header
  height 70px
  padding 2px 21px
.nav, .pagination, .carousel a
  cursor: pointer

.search
  width 250px
  display inline

.id
  border 1px solid #DDD;
.my_nac
  position absolute
  top 16%
  right 11%
.table-scrollable
  max-height 580px
  overflow-y scroll
  margin-top 65px

.products_list
  overflow-y scroll
  width 600px
  height 520px
  ul
    margin 10px 0
    li
      margin 10px 0

.sort
  a
    &:hover
      cursor pointer
a
  &:hover
    text-decoration none
h4
  margin 14px
